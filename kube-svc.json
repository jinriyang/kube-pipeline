{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "jfrog-cloud-svc",
    "namespace": "devops",
    "labels": {
      "app": "jfrog-cloud-svc"
    }
  },
  "spec": {
    "type": "NodePort",
    "selector": {
      "app": "jfrog-cloud-app"
    },
    "ports": [
      {
        "port": 80,
        "targetPort": 8080,
        "nodePort": 8181
      }
    ]
  }
}
